{
  "name": "@fbi-js/webpack-config-vue",
  "version": "1.3.0",
  "description": "Webpack config for Vue.js project",
  "homepage": "https://github.com/fbi-js/config/tree/master/packages/webpack-config-vue",
  "repository": "fbi-js/config",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "tsc --preserveWatchOutput -w",
    "build": "npm run clean && tsc -b .",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "format": "prettier -cw src/*.ts && npx eslint src/*.ts --fix"
  },
  "keywords": [],
  "author": "fbi",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@fbi-js/eslint-config-vue-typescript": "^1.2.0",
    "@fbi-js/stylelint-config": "^1.2.0",
    "@fbi-js/tsconfig": "^1.2.0",
    "@fbi-js/webpack-config-base": "^1.5.0",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.12"
  },
  "devDependencies": {
    "webpack": "^5.17.0"
  },
  "eslintConfig": {
    "extends": [
      "../eslint-config-typescript"
    ]
  }
}
